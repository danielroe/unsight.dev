    "@octokit/rest": "^21.0.2",
    "@unocss/nuxt": "^0.65.1",
    "hex-rgb": "^5.0.0",
    "ml-distance": "^4.0.1",
    "ml-kmeans": "^6.0.0",
    "nuxt": "latest",
    "nuxt-time": "1.0.3",
    "ohash": "^1.1.4",
    "openai": "^4.76.0",
    "unocss": "^0.65.1",
    "unplugin-vue-router": "^0.10.9"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.7.2",
    "lint-staged": "^15.2.10",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "vue-tsc": "^2.1.10",
    "wrangler": "^3.93.0"
