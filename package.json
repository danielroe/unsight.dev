    "@octokit/auth-app": "^7.1.3",
    "@octokit/rest": "^21.0.2",
    "@octokit/webhooks-types": "^7.6.1",
    "@unocss/nuxt": "^0.65.1",
    "hex-rgb": "^5.0.0",
    "ml-distance": "^4.0.1",
    "ml-kmeans": "^6.0.0",
    "nuxt": "latest",
    "nuxt-time": "1.0.3",
    "nuxt-webhook-validators": "0.1.5",
    "ohash": "^1.1.4",
    "rgb-to-hsl": "^0.0.3",
